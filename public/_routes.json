{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/assets/*",
    "/images/*",
    "/*.ico",
    "/*.svg",
    "/*.png",
    "/*.jpg",
    "/*.jpeg",
    "/*.gif"
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": ".*",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
