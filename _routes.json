{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/assets/*",
    "/images/*",
    "/*.ico",
    "/*.svg",
    "/*.png"
  ],
  "routes": [
    { "src": "/blog/.*", "dest": "/index.html" },
    { "src": "/blog", "dest": "/index.html" },
    { "src": ".*", "dest": "/index.html" }
  ]
}
